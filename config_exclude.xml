<config>
	<delimeter_setting>,</delimeter_setting>

	<input_setting>
		<!-- Priority of input is based on included format -->
		<included>
			<SenderPhoneNumber/>
			<DateSentTimeStamp/>
			<RecepientPhoneNumber/>
		</included>

		<!-- To exclude certain fields, Put it inside <excluded> tag  -->
		<excluded>
			<TimeStamp/>
			<DateReceivedTimeStamp/>
		</excluded>

	</input_setting>

	<!-- Priority of output is based on included format -->
	<output_setting>
		<included>
			<PhoneNumber/>
			<TotalSent/>
			<TotalReceived/>
		</included>

		<!-- To exclude certain fields, Put it inside <excluded> tag  -->
		<excluded>
			<DayHighestDifference/>
			<NumberHighestSent/>
			<DayHighestSent/>
		</excluded>
	</output_setting>
</config>
